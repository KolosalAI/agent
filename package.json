{
  "name": "@kolosal-ai/api-server-workspace",
  "version": "0.1.3",
  "description": "Kolosal AI API Server Workspace",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "workspaces": [
    "packages/api-server",
    "packages/core",
    "packages/test-utils"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KolosalAI/kolosal-cli.git"
  },
  "scripts": {
    "start": "npm run start --workspace=@kolosal-ai/api-server",
    "start:server": "npm run start:server --workspace=@kolosal-ai/api-server",
    "start:server:debug": "npm run start:server:debug --workspace=@kolosal-ai/api-server",
    "build": "npm run build --workspaces",
    "build:api-server": "npm run build --workspace=@kolosal-ai/api-server",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.9.0",
    "@types/shell-quote": "^1.7.5",
    "@types/uuid": "^10.0.0",
    "mime-types": "^3.0.1",
    "mock-fs": "^5.5.0",
    "msw": "^2.12.0",
    "read-package-up": "^11.0.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "license": "Apache-2.0"
}
